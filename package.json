{
  "name": "@olos/api",
  "version": "1.2.5",
  "description": "description",
  "author": "",
  "license": "MIT",
  "scripts": {
    "build": "node --max-old-space-size=1500 ./node_modules/.bin/tsc -p tsconfig.build.json",
    "format": "prettier --write \"src/**/*.ts\"",
    "start": "TZ=UTC ts-node src/main.ts",
    "start:dev": "cross-env TZ=UTC nodemon",
    "start:debug": "TZ=UTC nodemon --config nodemon-debug.json",
    "start:prod": "TZ=UTC node dist/main.js",
    "lint": "TZ=UTC tslint -p tsconfig.json -c tslint.json",
    "test": "cross-env TZ=UTC NODE_ENV=test mocha --timeout 10000 --require ts-node/register --exit src/**/**/*.spec.ts",
    "test:e2e": "cross-env TZ=UTC NODE_ENV=test mocha --timeout 10000 --require ts-node/register --exit --file e2e/setup.ts e2e/**/*.e2e.ts"
  },
  "dependencies": {
    "@nestjs/common": "7.0.8",
    "@nestjs/core": "7.0.8",
    "@nestjs/graphql": "7.3.4",
    "@nestjs/jwt": "^7.0.0",
    "@nestjs/mongoose": "^6.4.0",
    "@nestjs/passport": "^7.0.0",
    "@nestjs/platform-express": "7.0.8",
    "@nestjs/platform-socket.io": "7.0.8",
    "@nestjs/swagger": "4.5.3",
    "@nestjs/terminus": "7.0.1",
    "@nestjs/websockets": "7.0.8",
    "@octokit/graphql": "^4.3.1",
    "@octokit/rest": "17.5.2",
    "@types/mocha": "^7.0.2",
    "@types/sinon": "^9.0.0",
    "class-transformer": "^0.2.3",
    "class-validator": "0.12.1",
    "compression": "^1.7.4",
    "envalid": "^6.0.1",
    "fs-extra": "^9.0.0",
    "jsonwebtoken": "^8.5.1",
    "lodash": "^4.17.15",
    "mocha": "^7.1.2",
    "nest-raven": "^5.0.0",
    "normalize-url": "^5.0.0",
    "reflect-metadata": "^0.1.12",
    "rimraf": "3.0.2",
    "rxjs": "6.5.5",
    "semver": "7.3.2",
    "showdown": "1.9.1",
    "sinon": "^9.0.2",
    "socket.io-redis": "^5.2.0",
    "swagger-ui-express": "^4.1.4",
    "typescript": "^3.8.3"
  },
  "devDependencies": {
    "@nestjs/testing": "^7.0.6",
    "@types/chai": "^4.1.7",
    "@types/dotenv": "^8.2.0",
    "@types/express": "^4.16.0",
    "@types/fs-extra": "^8.1.0",
    "@types/node": "^13.1.4",
    "@types/passport-github": "^1.1.5",
    "@types/passport-google-oauth": "^1.0.39",
    "@types/passport-jwt": "^3.0.1",
    "@types/semver": "^7.1.0",
    "nodemon": "^1.18.10",
    "ts-loader": "^4.4.2",
    "ts-node": "^7.0.1",
    "tsconfig-paths": "^3.8.0",
    "tslint": "5.11.0"
  }
}
